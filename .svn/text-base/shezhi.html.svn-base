<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<meta charset="UTF-8">
		<title></title>
		<script src="js/rem.js" type="text/javascript" charset="utf-8"></script>
		<link rel="stylesheet" type="text/css" href="css/shezhi.css"/>
	</head>
	<body>
		<div class="wrapper">
			<div class="header">
				<div class="header-aside mui-action-back">
					<img src="img/youjian.png"/>
				</div>
				<div class="header-text">设置</div>	
				<div class="header-aside"></div>
			</div>
			<div class="main">
				<div class="main-one gerenxinxi">
					<div class="one-lift">
						<img src="img/945421.jpg" class="touxaing"/>
					</div>
					<div class="one-content">
						<div class="content-text name">新用户</div>
					</div>
					<div class="one-right">
						<img src="img/you-hui.png"/>
					</div>
				</div>
				<div class="main-two">
					<div class="two-top fankui">
						<div class="two-text">问题反馈</div>
						<img src="img/you-hui.png"/>
					</div>
					<div class="two-top tixian">
						<div class="two-text">设置提现密码</div>
						<img src="img/you-hui.png"/>
					</div>
				</div>
			</div>
			<div class="bottom">
				<div class="bottom-text">退出当前账户</div>
			</div>
		</div>
	</body>
	<script src="js/jquery.min.js" type="text/javascript" charset="utf-8"></script>
	<script src="js/mui.js" type="text/javascript" charset="utf-8"></script>
	<script src="js/URL.js" type="text/javascript" charset="utf-8"></script>
	<script type="text/javascript">
		$(function(){
			//			替换头像
			if(localStorage.getItem('smUsHeadimg') != 'null') {
				$('.touxaing').attr('src',myurl +  localStorage.getItem('smUsHeadimg'))
			}
			//			替换名字
			if(localStorage.getItem('name') != 'null') {
				$('.name').html(localStorage.getItem('name'))
			}
//			退出登录
			$('.bottom').click(function(){
				localStorage.clear()
				mui.openWindow({
					url: 'denglu.html',
					id: 'denglu'
				})
			})
			//			退出登录
			$('.fankui').click(function(){
				mui.openWindow({
					url: 'wentifankui.html',
					id: 'wentifankui'
				})
			})
			$('.gerenxinxi').click(function(){
				mui.openWindow({
					url: 'gerenziliao.html',
					id: 'gerenziliao'
				})
			})
			$('.tixian').click(function(){
				mui.openWindow({
					url: 'xtixian.html',
					id: 'xtixian'
				})
			})
		})
	</script>
</html>
