*{padding: 0;margin: 0;}
html,body,.wrapper{width: 100%;height: 100%;}
.wrapper{
	background: #F4F7FA ;
}
.header{
	height: .96rem;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 0 .3rem ;
	background: #FFFFFF ;
	margin-bottom: .1rem;
}
.header-aside{
	display: flex;
	align-items: center;
	justify-content: center;
	width: .3rem;
}
.header-aside img{
	height: .36rem;
}
.header-text{
	font-size: .36rem;
	color: #232323 ;
}
.main-one{
	display: flex;
	align-items: center;
	justify-content: flex-start;
	height: 1.6rem;
	background: #FFFFFF;
	padding: 0 .3rem ;
	margin-bottom: .2rem;
}
.one-lift{
	height: 1rem;
	width: 1rem;
	overflow: hidden;
	border-radius: 50%;
}
.one-lift img{
	width: 100%;
}
.one-content{
	flex: 1;
}
.content-text{
	font-size: .28rem;
	color: #272727 ;
	margin-left: .4rem;
}
.one-right{
	display: flex;
	align-items: center;
	justify-content: center;
}
.one-right img{
	height: .36rem;
}
.main-two{
	background: #FFFFFF;
	padding: 0 .3rem;
}
.two-top{
	display: flex;
	align-items: center;
	justify-content: space-between;
	height: .9rem;
}
.two-text{
	font-size: .28rem;
	color: #272727 ;
}
.two-top img{
	height: .36rem;
}
.bottom{
	height: 1rem;
	width: 100%;
	background: #c30e21 ;
	display: flex;
	align-items: center;
	justify-content: center;
	position: fixed;
	bottom: 0;
}
.bottom-text{
	font-size: .32rem;
	color: #FFFFFF ;
}






