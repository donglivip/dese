*{padding: 0;margin: 0;box-sizing: border-box;}
html,body,.wrapper{width: 100%;height: 100%;overflow: hidden;}
.wrapper{
	background: #F7F7F9 ;
}
.header{
	height: .96rem;
	background: #FFFFFF;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 0 .3rem;
}
.header-aside{
	width: .3rem;
	display: flex;
	align-items: center;
}
.header-aside img{
	height: .36rem;
}
.header-text{
	font-size: .36rem;
	color: #272727 ;
}
.main{height: calc(100% - .96rem);}
.main-three{
	background: #FFFFFF;
	margin: .2rem .3rem .2rem .3rem;
	border-radius: .12rem;
	padding: 0 .3rem;
}
.three-box{
	display: flex;
	align-items: center;
	justify-content: space-between;
	height: 1rem;
	border-bottom: 1px solid gainsboro;
}
.three-text{
	font-size: .28rem;
	color: #2B2B2B ;
}













